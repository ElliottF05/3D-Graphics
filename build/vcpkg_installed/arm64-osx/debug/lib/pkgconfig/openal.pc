prefix=${pcfiledir}/../..
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/../include

Name: OpenAL
Description: OpenAL is a cross-platform 3D audio API
Version: 1.23.1

Libs: "-L${libdir}" -lopenal -pthread -Wl,-framework,AudioToolbox -Wl,-framework,AudioUnit,-framework,ApplicationServices -Wl,-framework,CoreAudio -ldl -lm
Requires: 
Cflags: "-I${includedir}" "-I${includedir}/AL" -DAL_LIBTYPE_STATIC
